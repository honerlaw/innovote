{
  "name": "@onerlaw/innovote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run generate",
    "clean": "rm -rf .next *.log next-env.d.ts .swc coverage",
    "clean:install": "npm run clean && rm -rf package-lock.json node_modules && npm i",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "prisma generate",
    "vercel-build": "npm run generate && prisma migrate deploy && npm run build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier:write": "prettier --write --no-semi .",
    "format": "npm run lint:fix && npm run prettier:write && prisma format",
    "test": "jest"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.10.6",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.3.0",
    "@tailwindcss/postcss": "^4.0.3",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "newrelic": "^12.11.3",
    "next": "15.1.6",
    "next-logger": "^5.0.1",
    "pino": "^9.6.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@newrelic/test-utilities": "^9.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/newrelic": "^9.14.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prisma": "^6.3.0",
    "tailwindcss": "^4.0.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
